{
  "version": 2,
  "buildCommand": "cd project/backend && pip install -r requirements.txt",
  "outputDirectory": "project/backend",
  "builds": [
    {
      "src": "project/backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "project/backend/main.py"
    }
  ]
}
